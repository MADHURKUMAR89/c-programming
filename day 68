#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];
    long long sum = 0;


    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }

   
    long long expected_sum = (long long)n * (n + 1) / 2;

  
    int missing = expected_sum - sum;

    printf("%d", missing);

    return 0;
}
