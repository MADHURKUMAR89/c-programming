#include <stdio.h>

int main() {
    int n;


    printf("Enter the number of elements in the array: ");
    scanf("%d", &n);

    int arr[n];


    printf("Enter %d integers:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Previous Greater Elements:\n");


    for (int i = 0; i < n; i++) {
        int prevGreater = -1;  // Default if no greater element exists

        // Check for previous greater element on the left
        for (int j = i - 1; j >= 0; j--) {
            if (arr[j] > arr[i]) {
                prevGreater = arr[j];
                break;  // Stop at the first greater element on the left
            }
        }

        if (i == n - 1) {
            printf("%d\n", prevGreater);  // No comma after last element
        } else {
            printf("%d, ", prevGreater);
        }
    }

    return 0;
}
