#include <stdio.h>
#include <string.h>

int main() {
    char s[1000];  
    printf("Enter a string: ");
    scanf("%s", s);

    int n = strlen(s);
    int freq[256] = {0}; 
    int maxLen = 0;
    int start = 0;

    for (int end = 0; end < n; end++) {
        freq[(int)s[end]]++; 
        while (freq[(int)s[end]] > 1) {
            freq[(int)s[start]]--;
            start++;
        }


        int windowLen = end - start + 1;
        if (windowLen > maxLen) {
            maxLen = windowLen;
        }
    }

    printf("Length of the longest substring without repeating characters: %d\n", maxLen);

    return 0;
}
