#include <stdio.h>

int main() {
    long long num;
    int count[10] = {0}; // Array to store frequency of each digit
    int digit, maxDigit = 0, maxCount = 0;

    // Input the number
    printf("Enter an integer number: ");
    scanf("%lld", &num);


    if (num < 0) {
        num = -num;
    }


    while (num > 0) {
        digit = num % 10;
        count[digit]++;
        num /= 10;
    }
    for (int i = 0; i < 10; i++) {
        if (count[i] > maxCount) {
            maxCount = count[i];
            maxDigit = i;
        }
    }

    printf("Digit %d occurs the most (%d times).\n", maxDigit, maxCount);

    return 0;
}
